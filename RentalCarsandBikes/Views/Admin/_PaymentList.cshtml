@model List<BAL.ViewModel.MoneyTransactionListViewBAL>

<i class="fa fa-arrow-circle-o-left" aria-hidden="true" onclick='window.location="@Url.ActionLink("Index","Admin")";' style="top: 100px;position: absolute;">Back</i>

<div class="container" style="top: 167px;position: absolute; left: 150px;">

    <table class="table table-bordered" id="myTable" cellpadding="0" cellspacing="0">
        <thead>
            <tr>
                <th>Transactio Id</th>              
                <th>Name</th>               
                <th>Total Days</th>              
                <th>Vehicle Name</th>
                <th>Transaction Amount</th>
                <th>User Id</th>
                <th>Booking Id</th>
            </tr>
        </thead>
        <tbody id="datalist">
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.TransactionId</td>
                    <td>@item.Name</td>
                    <td>@item.TotalDays</td>
                    <td>@item.VehicleName</td>
                    <td>@item.TransactionAmount</td>
                    <td>@item.UserId</td>
                    <td>@item.BookingId</td>
                </tr>
            }
        </tbody>
    </table>
</div>